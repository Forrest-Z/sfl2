all: Makefile QtNepumuk.qmake
	$(MAKE) -f QtNepumuk.qmake

QtNepumuk.qmake: QtNepumuk.pro
	qmake -makefile -o QtNepumuk.qmake QtNepumuk.pro

clean:
	- rm -rf *~ *.qmake *.o *.app moc_*.cpp

# fake targets to sort of integrate with the Automake setup

distdir:
	@ echo "nepumuk/qt is too experimental for packaging" > README.qt
